{
  "name": "breezeframe-v1",
  "version": "2.1.0",
  "private": true,
  "description": "BreezeFrame - AI Window Analysis & Smart Home Integration",
  "keywords": [
    "ai",
    "tensorflow",
    "window-analysis",
    "smart-home",
    "computer-vision",
    "nextjs",
    "supabase",
    "python",
    "opencv"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/breezeframe/breezeframe-v1"
  },
  "license": "MIT",
  "author": "BreezeFrame Team",
  "scripts": {
    "auto-configure": "tsx scripts/auto-configure.ts",
    "auto-setup": "tsx scripts/auto-setup.ts",
    "build": "next build",
    "clean": "rm -rf .next node_modules/.cache logs/*.log",
    "clean-win": "rmdir /s /q .next && rmdir /s /q node_modules\\.cache && del logs\\*.log",
    "dataset-import": "tsx scripts/run-dataset-import.ts",
    "dev": "next dev",
    "postinstall": "echo '✅ Installation terminée. Lancez: npm run setup-all'",
    "install-python-backend": "tsx scripts/install-python-backend.ts",
    "install-tensorflow": "tsx scripts/install-tensorflow-cli.ts",
    "lint": "next lint",
    "python-backend": "cd python-backend && ./start_server.sh",
    "python-backend-win": "cd python-backend && start_server.bat",
    "run-import": "tsx scripts/run-import.ts",
    "setup-all": "tsx scripts/quick-start.ts",
    "setup-database": "tsx scripts/setup-database.ts",
    "start": "next start",
    "test-ai": "tsx scripts/test-ai-system.ts",
    "test-first": "tsx scripts/run-first-tests.ts",
    "test-imports": "tsx scripts/test-imports.ts",
    "type-check": "tsc --noEmit"
  },
